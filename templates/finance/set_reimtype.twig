{# empty Twig template #}
<?xml version="1.0" encoding="UTF-8"?>
<script src="{{url}}js/cert/set_cert.js" type="text/javascript"></script>
<style>
    .error{
        color:red;
    }
</style>
<div>
    <div>
        <iframe name="myiframe" style="display:none;" onload="iframeLoad(this);"></iframe>
        <form action="../../modules/finance/action/actionReimType.php" method="post"
              enctype='multipart/form-data' target="myiframe" id="commentForm" >
            <div class="set_content_tb">
                <table class="tb_set" >
                    <tr>
                        <td>报销类别</td>
                        <td class="td_left"><input  id="reim_type" name="reim_type" class="input_text" type="text" value="{{info.ReimTypeName}}" required></td >
                    </tr>   
                    <tr>
                        <td>报销科目</td>
                        <td  class="td_left">
                            <textarea {{readonly}} id="reim_sub" name="reim_sub"  class="input_textarea" type="text">{{info.ReimSubName}}</textarea>
                        </td>
                    </tr>
                </table>
                <div class="bottom_button">
                    <input type="text" id="id" name="id" hidden="hidden" value="{{info.Id}}"/>
                    <input type="submit" name="{{btn}}"  class="comm_button button_save" value="保存"/>
                    <input type="button" onclick="toPage('../../modules/finance/allReimType.php?pagetype={{pagetype}}')" name="close" class="comm_button button_close" value="关闭"/>
                </div>
            </div>
        </form>
    </div>
</div>


